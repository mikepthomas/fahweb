<!DOCTYPE hibernate-mapping PUBLIC '-//Hibernate/Hibernate Mapping DTD 3.0//EN' 'http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd'>

<hibernate-mapping package="config.hibernate">
    <class name="com.googlecode.fahweb.model.Team" table="daily_team_summary">
        <id name="uniqueId" column="ID">
            <generator class="native"/>
        </id>
        <property name="team"/>
        <property name="teamName" column="teamname"/>
        <property name="score"/>
        <property name="wu"/>
    </class>
</hibernate-mapping>
