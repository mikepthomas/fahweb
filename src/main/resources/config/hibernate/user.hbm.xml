<!DOCTYPE hibernate-mapping PUBLIC '-//Hibernate/Hibernate Mapping DTD 3.0//EN' 'http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd'>

<hibernate-mapping package="config.hibernate">
    <class name="info.mikethomas.fahweb.model.User" table="daily_user_summary">
        <id name="uniqueId" column="ID">
            <generator class="native"/>
        </id>
        <property name="name"/>
        <property name="newCredit" column="newcredit"/>
        <property name="sum"/>
        <property name="team"/>
    </class>
</hibernate-mapping>
